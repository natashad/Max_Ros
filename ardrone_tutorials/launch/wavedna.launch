<launch>
  <node pkg="vicon_bridge" type="vicon_bridge" name="vicon" output="screen">
    <param name="stream_mode" value="ClientPull" type="str" />
    <param name="datastream_hostport" value="192.168.2.102:801" type="str" />
    <param name="ARDroneShare/ARDroneShare/zero_pose/orientation/x" value="0.675575072339" type="double" /> 
    <param name="ARDroneShare/ARDroneShare/zero_pose/orientation/y" value="-0.04463231523" type="double" /> 
    <param name="ARDroneShare/ARDroneShare/zero_pose/orientation/z" value="0.734692986763" type="double" />
    <param name="ARDroneShare/ARDroneShare/zero_pose/orientation/w" value="-0.0428076310194" type="double" />

    <param name="ARDroneShare/ARDroneShare/zero_pose/position/x"    value="0.116261226836" type="double" />
    <param name="ARDroneShare/ARDroneShare/zero_pose/position/y"    value="-0.0281664390969" type="double" />
    <param name="ARDroneShare/ARDroneShare/zero_pose/position/z"    value="0.01" type="double" />
  </node>
  <node name="vicon_coordinates" pkg="ardrone_tutorials" type="vicon.py" output="screen" required="True" /> 
  <node name="dsl_controller" pkg="ardrone_tutorials" type="dsl_controller.py" output="screen" required="True" />
  <node name="wavedna" pkg="ardrone_tutorials" type="wavedna.py" output="screen" required="True" />
</launch>

